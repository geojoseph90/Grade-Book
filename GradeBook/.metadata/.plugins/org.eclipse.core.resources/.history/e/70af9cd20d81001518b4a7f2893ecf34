package com.gradebook.DO;

import java.util.List;

public class Student {
	private String name = null;
	private String id = null;
	private List<AssignedWork> Assigned_Work = null;
	private String finalGrade = null;

	public String getStudentName() {
		return name;
	}
	public void setStudentName(String studentName) {
		this.name = studentName;
	}
	public String getStudentId() {
		return id;
	}
	public void setStudentId(String studentId) {
		this.id = studentId;
	}
	public List<AssignedWork> getAssignedWork() {
		return Assigned_Work;
	}
	public void setAssignedWork(List<AssignedWork> generalAssignedWork) {
		this.Assigned_Work = generalAssignedWork;
	}
	public String getFinalGrade() {
		return finalGrade;
	}
	public void setFinalGrade(String finalGrade) {
		this.finalGrade = finalGrade;
	}
	
}
